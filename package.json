{
  "name": "l5r4-migrator",
  "version": "1.0.0",
  "description": "Migration tool for transferring worlds from legacy l5r4 system to l5r4-enhanced system",
  "type": "module",
  "author": "Ernie Ayala",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ernieayala/l5r4-migrator.git"
  },
  "bugs": {
    "url": "https://github.com/ernieayala/l5r4-migrator/issues"
  },
  "homepage": "https://github.com/ernieayala/l5r4-migrator#readme",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint module/ tests/",
    "lint:fix": "eslint module/ tests/ --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "validate": "npm run lint && npm run format:check && npm test"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "happy-dom": "^12.10.3",
    "prettier": "^3.2.5",
    "vitest": "^1.0.0"
  }
}
